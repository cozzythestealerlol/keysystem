copyBtn.MouseButton1Click:Connect(function()
	if setclipboard then
		setclipboard(clipboardLink)
		copyBtn.Text = "Copied!"
		task.wait(2)
		copyBtn.Text = "Key System Here"
	end
end)

------------------------------------------------------------
-- FADE-IN EFFECT
------------------------------------------------------------

frame.Visible = true
frame.BackgroundTransparency = 1
bgGradient.Transparency = NumberSequence.new(1)
blur.Size = 0

task.spawn(function()
	for i = 1, 20 do
		frame.BackgroundTransparency = 1 - (i * 0.02)
		blur.Size = i * 0.6
		bgGradient.Transparency = NumberSequence.new(1 - (i * 0.05))
		task.wait(0.02)
	end
end)

------------------------------------------------------------
-- KEY VALIDATION WITH ANIMATION + FADE-OUT
------------------------------------------------------------

local function resetKeyBox()
	keyBox.PlaceholderText = "Enter your key"
	keyBox.Text = ""
	submitBtn.Active = true
	keyBox.Active = true
end

-- Helper to finish the success flow: cache the key (if possible), show welcome fade, then run script
local function successFlow()
	-- cache the accepted key locally (if file API available)
	local expiry = os.time() + CACHE_SECONDS
	writeCache(correctKey, expiry)

	-- Animate to center and shrink
	local tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	TweenService:Create(frame, tweenInfo, {
		Size = UDim2.new(0, 250, 0, 120),
		Position = UDim2.new(0.5, 0, 0.5, 0)
	}):Play()

	-- Clear contents (remove children)
	for _, obj in ipairs(frame:GetChildren()) do
		obj:Destroy()
	end

	-- Welcome label
	local welcomeText = Instance.new("TextLabel")
	welcomeText.Parent = frame
	welcomeText.Size = UDim2.new(1, 0, 1, 0)
	welcomeText.BackgroundTransparency = 1
	welcomeText.TextColor3 = Color3.fromRGB(255, 255, 255)
	welcomeText.Font = Enum.Font.GothamBold
	welcomeText.TextScaled = true
	welcomeText.Text = "Welcome, " .. player.Name .. "!"

	task.wait(1.25)

	-- Fade out entire frame smoothly (frame transparency + text + reduce blur)
	for i = 1, 20 do
		local alpha = i * (1/20)
		frame.BackgroundTransparency = 0.55 + (0.45 * alpha)
		welcomeText.TextTransparency = alpha
		-- gradually reduce blur (avoid multiplying by alpha repeatedly; step down)
		blur.Size = math.max(0, blur.Size - (0.6 * (1/20) * 20)) -- fallback decrement
		task.wait(0.03)
	end

	-- Execute the actual script after fade
	local scriptURL = gameScripts[game.PlaceId] or defaultScript
	runGameScriptAndCleanup(scriptURL, gui, blur)
end

submitBtn.MouseButton1Click:Connect(function()
	local entered = keyBox.Text

	if entered == correctKey then
		-- success path
		successFlow()
	else
		-- wrong key feedback
		keyBox.PlaceholderText = "Wrong Key Was Entered, Try Again!"
		keyBox.Text = ""
		submitBtn.Active = false
		keyBox.Active = false

		task.delay(2, function()
			resetKeyBox()
		end)
	end
end)
